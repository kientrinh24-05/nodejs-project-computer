<div class="ps-page--default ps-page--contact">
    <div class="ps-page__header bg--top" data-background="/page/img/bg/contact-bg.jpg">
      <div class="container">
        <h3>Contact</h3>
        <ul class="ps-breadcrumb">
          <li><a href="#">Home</a></li>
          <li>Contact</li>
        </ul>
      </div>
    </div>
    <div class="ps-page__content">
      <div class="container">
        <div class="ps-contact">
          <div class="ps-section__left">
            <form class="ps-form--contact" action="/lien-he" method="post">
              <div class="row">
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 ">
                  <div class="form-group">
                    <label>Họ tên <sup>*<sup/></label>
                    <input required class="form-control" type="text" placeholder="" name="fullname">
                  </div>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 ">
                  <div class="form-group">
                    <label>Email<sup>*<sup/></label>
                    <inputrequired  class="form-control" type="email" placeholder="" name="email">
                  </div>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 ">
                  <div class="form-group">
                    <label>Điện thoại<sup>*<sup/></label>
                    <inputrequired  class="form-control" type="text" placeholder="" name="phone">
                  </div>
                </div>
              </div>
              <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 ">
                <div class="form-group">
                  <label>Chủ đề<sup>*<sup/></label>
                  <input required class="form-control" type="email" placeholder="" name="subject">
                </div>
              </div>
              <div class="form-group">
                <label>Nội dung<sup>*<sup/></label>
                <textarea required class="form-control" rows="6" placeholder="" name="content"></textarea>
              </div>
              <div class="form-group submit">
                <button class="ps-btn ps-btn--black">Gửi</button>
              </div>
            </form>
          </div>
          <div class="ps-section__right">
            <ul class="ps-list--social">
              <li><a href="#"><i class="fa fa-facebook"></i></a></li>
              <li><a href="#"><i class="fa fa-twitter"></i></a></li>
              <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
              <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
              <li><a href="#"><i class="fa fa-instagram"></i></a></li>
            </ul>
            <figure>
